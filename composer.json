{
    "name": "esc-company/good-code-parser",
    "description": "ESC-Company suggests general code-spec, parser and implement spec.",
    "keywords": [
        "composer",
        "php",
        "esc-company",
        "holapet",
        "psr-12",
        "packagist"
    ],
    "type": "project",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Lee",
            "email": "cable8mm@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "EscCompany\\GoodCodeParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "start": [
            "@php bin/init.php"
        ],
        "test": [
            "./vendor/bin/phpunit tests"
        ]
    }
}